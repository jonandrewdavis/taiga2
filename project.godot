NetworkPerformance="*res://addons/netfox/network-performance.gd"
Async="*res://examples/shared/scripts/async.gd"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=2073
window/size/viewport_height=1167
window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/vsync/vsync_mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/netfox.internals/plugin.cfg", "res://addons/netfox.noray/plugin.cfg", "res://addons/netfox/plugin.cfg", "res://addons/script-ide/plugin.cfg")

