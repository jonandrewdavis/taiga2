config_version=5

[application]

config/name="ForestBath"
config/version="2.0.0"
run/main_scene="res://level/scenes/level.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/max_fps=60
config/icon="res://forest_bath.png"

[audio]

buses/default_bus_layout="res://audio/default_bus_layout.tres"

[autoload]

Network="*res://level/scripts/network.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
Hub="*res://utility scripts/hub.gd"
Noray="*res://addons/netfox.noray/noray.gd"
PacketHandshake="*res://addons/netfox.noray/packet-handshake.gd"
